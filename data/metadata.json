{
	"dataflows": [{
		"name": "prueba-acceso",
		"sources": [{
			"name": "person_inputs",
			"path": "data/input/events/person/",
			"format": "JSON"
		}],
		"transformations": [{
				"name": "validation",
				"type": "validate_fields",
				"params": {
					"input": "person_inputs",
					"validations": [{
							"field": "office",
							"validations": ["notEmpty"]
						},
						{
							"field": "age",
							"validations": ["notNull"]
						}
					]
				}
			},
			{
				"name": "ok_with_date",
				"type": "add_fields",
				"params": {
					"input": "person_inputs",
					"addFields": [{
						"name": "dt",
						"function": "current_timestamp"
					}]
				}
			}
		],
		"sinks": [{
				"input": "person_inputs",
				"name": "raw-ok",
				"paths": [
					"data/output/events/person/"
				],
				"format": "JSON",
				"saveMode": "OVERWRITE",
				"options": {
					"ignoreNullFields": false
				}
			},
			{
				"input": "validation_ko",
				"name": "raw-ko",
				"paths": [
					"data/output/discards/person/"
				],
				"format": "JSON",
				"saveMode": "OVERWRITE",
				"options": {
					"ignoreNullFields": false
				}
			}
		]
	}]
}